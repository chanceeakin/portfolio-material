{
  "name": "react-md-redux",
  "version": "0.0.1",
  "description": "A boilerplate repo for setting up an isomorphic React project with react-md and redux.",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "better-npm-run start:prod",
    "build": "npm run clean && better-npm-run build",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint -c .eslintrc ./src",
    "lint:styles": "stylelint \"./src/**/*.?(s)css\"",
    "clean": "rimraf dist/!\\(favicon.ico\\)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md-redux.git"
  },
  "keywords": [
    "react-md",
    "boilerplate",
    "redux",
    "react"
  ],
  "author": "Mikkel Laursen (mlaursen03@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md-redux/issues"
  },
  "homepage": "https://github.com/mlaursen/react-md-redux#readme",
  "betterScripts": {
    "start": {
      "command": "nodemon ./index.js",
      "env": {
        "NODE_ENV": "development",
        "NODE_PORT": 3000
      }
    },
    "start:prod": {
      "command": "node ./index.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PORT": 8080
      }
    },
    "build": {
      "command": "webpack --progress --display-error-details --config ./configs/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "better-npm-run": "0.0.13",
    "bluebird": "^3.4.6",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "ejs": "^2.5.3",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "hpp": "^0.2.1",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.7.0",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.3",
    "react-hot-loader": "3.0.0-beta.6",
    "react-md": "^1.0.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2",
    "vhost": "^3.0.2",
    "webpack-isomorphic-tools": "^2.6.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.1.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
